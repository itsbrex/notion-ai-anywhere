{
  "name": "notion-ai-anywhere",
  "displayName": "Notion AI Anywhere",
  "version": "1.0.0",
  "description": "notion-ai-anywhere",
  "author": "smackgg",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.16",
    "@notionhq/client": "^2.2.3",
    "@paddlejs-models/ocr": "^1.2.3",
    "@plasmohq/messaging": "^0.1.6",
    "@plasmohq/storage": "^1.3.1",
    "@tryfabric/martian": "^1.2.4",
    "can-ndjson-stream": "^1.0.2",
    "classnames": "^2.3.2",
    "copy-to-clipboard": "^3.3.3",
    "daisyui": "^2.51.5",
    "node-html-markdown": "^1.3.0",
    "plasmo": "0.67.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shrink-string": "^3.1.2",
    "tesseract.js": "^4.0.2",
    "theme-change": "^2.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.0",
    "@types/chrome": "0.0.202",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "postcss-convert-rem-to-pixel": "^0.0.0",
    "prettier": "2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.3"
  },
  "manifest": {
    "default_locale": "en",
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "~tesseract-data/*.traineddata.gz",
          "~js/tesseract-core.wasm.js",
          "~js/worker.min.js"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "commands": {
      "show-panel": {
        "suggested_key": {
          "default": "Ctrl+K",
          "mac": "Command+K"
        },
        "description": "Activate NotionAIAnywhere on page"
      }
    }
  }
}
